{
	"name": "ThemeKit",
	"version": "1.0.0",
	"description": "The simplest WordPress starter theme including full setup for Sass, PostCSS, Autoprefixer, stylelint, Webpack, ESLint, imagemin, Browsersync, etc.",
	"scripts": {
		"dev": "mix watch --mix-config=./.dev/webpack.mix.js",
		"build": "mix  --mix-config=./.dev/webpack.mix.js",
		"build:production": "mix --production  --mix-config=./.dev/webpack.mix.js",
		"wp-env": "wp-env"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/atakanoz/themekit.git"
	},
	"keywords": [],
	"license": "GPL-2.0+",
	"homepage": "https://github.com/atakanoz/themekit",
	"devDependencies": {
		"@tailwindcss/line-clamp": "^0.4.4",
		"@tailwindcss/typography": "^0.5.9",
		"@wordpress/env": "^8.7.0",
		"autoprefixer": "^10.4.2",
		"browser-sync": "^2.27.7",
		"browser-sync-webpack-plugin": "^2.3.0",
		"copy-webpack-plugin": "^8.1.1",
		"imagemin-mozjpeg": "^8.0.0",
		"imagemin-webpack-plugin": "^2.4.2",
		"laravel-mix": "^6.0.41",
		"laravel-mix-copy-watched": "^2.3.1",
		"laravel-mix-glob": "^2.0.2",
